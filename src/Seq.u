class Seq
  include TaskBase, Stream

  def initialize(values)
    { values: values,
      task_list: []}
  end

  def initialize(values, task_list)
    { values: values,
      task_list: task_list }
  end

  def spawn_actor(next_actor)
    @values.each do |item|
      next_actor ! item
    end
    next_actor ! :eos
  end

  def update_task_list(task_list)
    new Seq(@values, task_list)
  end

end