class PyModule
  def initialize(module_name)
    {:ok, py_process} = python::start()
    {module_name: module_name, process: py_process}
  end

  def initialize(module_name, options)
    {:ok, py_process} = python::start(options)
    {module_name: module_name, process: py_process}
  end

  def method_missing(method_name, args)
    python::call(@process, @module_name, method_name, args)
  end

  def stop()
    python::stop(@process)
  end
end